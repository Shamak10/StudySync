<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!-- Primary Colors -->
    <Color x:Key="Primary">#007AFF</Color>
    <Color x:Key="PrimaryDark">#0056B3</Color>
    <Color x:Key="PrimaryLight">#E6F2FF</Color>
    <!-- This is the key that was not found -->

    <!-- Neutral Colors -->
    <Color x:Key="PageBackgroundColor">#F8F9FA</Color>
    <Color x:Key="CardBackgroundColor">#FFFFFF</Color>
    <Color x:Key="PrimaryTextColor">#212529</Color>
    <Color x:Key="SecondaryTextColor">#6C757D</Color>
    <Color x:Key="BorderColor">#E0E0E0</Color>

    <!-- Accent Colors -->
    <Color x:Key="AccentColor">#17A2B8</Color>
    <Color x:Key="DeleteColor">#DC3545</Color>

    <!-- Standard Colors -->
    <Color x:Key="White">#FFFFFF</Color>
    <Color x:Key="Black">#000000</Color>

</ResourceDictionary>